{
  "name": "contributte/utils",
  "description": "Extra contrib to nette/utils",
  "keywords": [
    "nette",
    "utils",
    "strings",
    "filesystem",
    "datetime"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/utils",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": "^7.2",
    "nette/utils": "~2.4.10 | ~2.5.2"
  },
  "require-dev": {
    "nette/di": "~2.4.13",
    "ninjify/nunjuck": "^0.2.0",
    "ninjify/qa": "^0.9.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11"
  },
  "suggest": {
    "nette/di": "to use DateTimeExtension[CompilerExtension]"
  },
  "conflict": {
    "nette/di": "<2.4.13"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Utils\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Contributte\\Utils\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
