{
  "name": "contributte/utils",
  "description": "Extra contrib to nette/utils",
  "keywords": [
    "nette",
    "utils",
    "strings",
    "filesystem",
    "datetime"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/utils",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "nette/utils": "^4.0.0"
  },
  "require-dev": {
    "contributte/phpstan": "^0.2",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.4",
    "mockery/mockery": "^1.6.0",
    "nette/di": "^3.2.4"
  },
  "suggest": {
    "nette/di": "to use DateTimeExtension[CompilerExtension]"
  },
  "conflict": {
    "nette/di": "<3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Utils\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.8.x-dev"
    }
  }
}
